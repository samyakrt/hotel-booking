{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes":["error","single"],
        "no-await-in-loop":"warn",
        "no-unreachable":"warn",
        "no-extra-semi":"error",
        "no-return-await":"warn",
        "semi":"error"
    }
}
